{
  "name": "tfjs-recycling",
  "version": "0.0.1",
  "description": "Image recognition for easier recycling",
  "author": "Charlie Gerard",
  "license": "MIT",
  "private": false,
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "watch": "webpack --watch",
    "start": "webpack-dev-server --open",
    "clean": "rm -rf public/bundle.js",
    "clean-all": "rm -rf public/bundle.js node_modules"
  },
  "dependencies": {
    "@tensorflow-models/coco-ssd": "^0.1.1",
    "@tensorflow-models/mobilenet": "^0.2.2",
    "@tensorflow/tfjs": "0.15.3",
    "lodash": "^4.17.11"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "webpack": "*",
    "webpack-cli": "*",
    "webpack-dev-server": "*"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "esmodules": false,
          "targets": {
            "browsers": [
              "> 1%",
              "last 3 versions",
              "ie >= 9",
              "ios >= 8",
              "android >= 4.2"
            ]
          },
          "useBuiltIns": "usage"
        }
      ]
    ]
  },
  "browser": { "fs": false, "path": false, "os": false}
}
